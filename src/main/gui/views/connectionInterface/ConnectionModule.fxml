<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="65.0" prefWidth="386.0">
         <children>
            <Label text="USB Connection :" />
            <HBox alignment="CENTER_LEFT" prefHeight="0.0" prefWidth="386.0">
               <children>
                  <ComboBox prefHeight="25.0" prefWidth="122.0" promptText="Available Ports" />
                  <Button mnemonicParsing="false" text="Connect">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Label text="Wait For Connection." />
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </HBox>
            <Button mnemonicParsing="false" text="Refresh" />
         </children>
      </VBox>
   </children>
</fx:root>
